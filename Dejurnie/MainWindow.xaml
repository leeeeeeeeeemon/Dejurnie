        <TextBox x:Name="RegBoxGroup" HorizontalAlignment="Left" Margin="57,123,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="60"/>
        <TextBox x:Name="FirstDejyrnii" HorizontalAlignment="Left" Margin="400,42,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160" Height="122"/>
        <TextBox x:Name="SecondDejurniy" HorizontalAlignment="Left" Margin="611,42,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160" Height="122"/>
        <TextBox x:Name="GroupToChoise" HorizontalAlignment="Left" Margin="400,199,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="getDejurniyButton" Content="Get Students
                " HorizontalAlignment="Left" Margin="584,199,0,0" VerticalAlignment="Top" Click="getDejurniyButton_Click"/>
    </Grid>
</Window>
